function receiveMessage(e){if("200"!==e.data.returnCode)return;let t=document.getElementsByTagName("iframe"),n=[];for(let a=0;a<t.length;a++)n.push(t[a].src),t[a].remove();window.parent.postMessage({returnCode:"299",msg:"iframe",data:n},"*");let s={};for(let l=0;l<e.data.data.length;l++){let r=e.data.data[l].imgUrl;s[r]=e.data.data[l].text}let o=document.getElementsByTagName("img");for(let g=0;g<o.length;g++){let d=o[g].src,m=s[d];m&&(o[g].alt=m)}}window.addEventListener("DOMContentLoaded",function(){window.parent.postMessage({returnCode:"288",msg:"DOMContentLoaded"},"*");let e=document.getElementsByTagName("img"),t=document.getElementsByTagName("body");t[0].style.WebkitTouchCallout="none",t[0].style.WebkitUserSelect="none",t[0].style.MozUserSelect="none",t[0].style.MsUserSelect="none",t[0].style.userSelect="none",t[0].style.WebkitUserDrag="none",t[0].style.MozUserDrag="none",t[0].style.MsUserDrag="none";for(let n=0;n<e.length;n++)e[n].style.pointerEvents="none"}),window.addEventListener("message",receiveMessage,!1);